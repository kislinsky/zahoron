{
    "classmap": [
        "vendor/google/apiclient/src",
        "vendor/dadata/apiclient/src"
    ],
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "artesaos/seotools": "^1.3",
        "ausi/slug-generator": "^1.1",
        "dadata/apiclient": "^2.0",
        "darkaonline/l5-swagger": "^8.6",
        "filament/actions": "^3.2",
        "filament/filament": "^3.2",
        "filament/forms": "^3.2",
        "filament/infolists": "^3.2",
        "filament/notifications": "^3.2",
        "filament/tables": "^3.2",
        "filament/widgets": "^3.2",
        "google/recaptcha": "^1.3",
        "guzzlehttp/guzzle": "^7.9",
        "harvesthq/chosen": "^1.8",
        "hflabs/dadata": "^24.4",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/scout": "^10.15",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.5",
        "movemoveapp/laravel-dadata": "^1.0",
        "nesbot/carbon": "^2.72",
        "nutnet/laravel-sms": "^0.11.0",
        "php-open-source-saver/jwt-auth": "^2.3",
        "phpmailer/phpmailer": "^6.10",
        "phpoffice/phpspreadsheet": "^3.3",
        "rap2hpoutre/fast-excel": "^5.5",
        "smsaero/smsaero_api": "^1.0",
        "spatie/laravel-sitemap": "^7.3",
        "staudenmeir/belongs-to-through": "^2.15",
        "tinymce/tinymce": "^7.6",
        "yoomoney/yookassa-sdk-php": "^3.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/lang": "^14.8",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/functions.php",
            "app/helpers-api.php",
            "app/pages.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
